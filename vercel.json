{
  "buildCommand": "pnpm turbo build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/messaging/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/collaboration/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
